<%-include("./layouts/header") %>
    <div class="container-scroller">
        <%-include("./layouts/menu") %>

            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="row">

                        <div class="col-md-12 grid-margin stretch-card">
                            <div class="card">
                              <div class="card-body">
                                <h4 class="card-title">Add Members</h4>

                                <form class="forms-sample" method="POST" action="/add-members">
                                  <div class="form-group">
                                    <label for="exampleInputUsername1">Full Name</label>
                                    <input type="text" name="fullName" class="form-control" id="exampleInputUsername1" placeholder="Full Name">
                                  </div>
                                  <div class="form-group">
                                    <label for="exampleInputEmail1">Mission</label>
                                    <input type="text" name="mission" class="form-control" id="exampleInputEmail1" placeholder="Mission">
                                  </div>
                                  <div class="form-group">
                                    <label for="exampleInputPassword1">Price</label>
                                    <input type="text" name="price" class="form-control" id="exampleInputPassword1" placeholder="Password">
                                  </div>

                                  <button type="submit" class="btn btn-primary mr-2">Submit</button>
                                </form>
                              </div>
                            </div>
                          </div>


                    </div>
                    <!-- main-panel ends -->
                </div>
                <!-- page-body-wrapper ends -->
            </div>
            <%-include("./layouts/footer") %>